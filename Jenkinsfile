pipeline {
   agent any

   stages {
      stage('Checkout') {
         steps {
            echo 'Checkout Hello World'
         }
      }
   
       stage('Create') {
         steps {
            echo 'Create Hello World'
         }
       }
       
       stage('Deploy') {
         steps {
            echo 'Deploy Hello World'
         }
       }
       
   }
}
